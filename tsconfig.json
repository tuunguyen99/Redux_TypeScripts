{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"],
        "form":["redux-toolkit/form/*"]
      }
    },

    "include": [
      "src/index.tsx"
    ]
  }
  